@model MVCRiceStore.Models.Client

@{
    ViewBag.Title = "Delete";
}

<h2>Delete</h2>

@using (Html.BeginForm()) { 
    <div class="form-horizontal">
        <div class="form-group">
            <div class="col-md-4">@Html.Label("Client Name")</div>
            <div class="col-md-8">@Html.DisplayFor(model=>model.Name)</div>
        </div>

        <div class="form-group">
            <div class="col-md-4">@Html.Label("Note")</div>
            <div class="col-md-8">@Html.DisplayFor(model=>model.Note)</div>
        </div>

        <div class="form-group">
            <input type="submit" value="Delete"/>
        </div>
    </div>
}


